{
  "$schema": "./local-schema/package.schema.json",
  "name": "oxa",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "expo lint",
    "apk:release": "powershell -ExecutionPolicy Bypass -File ./scripts/build-release.ps1",
    "apk:release-fast": "powershell -ExecutionPolicy Bypass -File ./scripts/build-release-fast.ps1",
    "apk:release-main": "cd android && gradlew.bat clean assembleRelease && cd .. && adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install-debug-apk": "adb install -r ./android/app/build/outputs/apk/debug/app-debug.apk",
    "install-release-apk": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "@types/react": "~19.1.10",
    "axios": "^1.13.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~54.0.33",
    "expo-av": "^16.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-camera": "^17.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "^14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-screen-capture": "^8.0.9",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-updates": "~29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.69.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-webrtc": "^124.0.7",
    "socket.io-client": "^4.8.3"
  },
  "private": true,
  "devDependencies": {
    "@react-native-community/cli": "latest",
    "typescript": "~5.9.2"
  }
}
